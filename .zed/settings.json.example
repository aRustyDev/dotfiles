    }
  },
  "load_direnv": "shell_hook",
  "disable_ai": true,
  "diagnostics_max_severity": "hint",
  "proxy": "socks5h://localhost:9876",
  "private_files": [
    "**/.priv*.env*",
    "**/*.pem",
    "**/*.key",
    "**/*.cert",
    "**/*.crt"
  ],
  "redact_private_values": true,
  "format_on_save": "on",
  "relative_line_numbers": true,
  "journal": {
    "path": "$XDG_CACHE_HOME/zed/journal",
    "hour_format": "hour12"
  },
  "languages": {
    "Markdown": {
      "format_on_save": "on"
    },
    "C": {
      "format_on_save": "on",
      "tab_size": 2
    },
    "Sieve": {
      "language_servers": ["sieve_lsp"],
      "tab_size": 2,
      "hard_tabs": false,
      "preferred_line_length": 80,
      "format_on_save": "on",
      "formatter": "language_server",
      "show_completions_on_input": true
    }
  },
  "file_types": {
    "Diff": ["dif"],
    "C": ["C", "H"],
    "Markdown": ["*.md", "*.mdc", "*.rules"],
    "Tide": ["*.tide", "*.td"],
    "Sieve": ["sieve", "sv", "*.sieve", "*.sv"],
    "Git Config": [
      "$XDG_CONFIG_HOME/git/config",
      "$XDG_CONFIG_HOME/git/configs/*"
    ],
    "SSH Config": [
      "$XDG_CONFIG_HOME/ssh/config",
      "$XDG_CONFIG_HOME/ssh/configs/*"
    ]
  },
  "lsp": {
    "sieve_lsp": {
      "binary": {
        "ignore_system_version": false,
        "path": "$HOME/.cargo/bin/sieve_lsp",
        "env": {
          "RUST_LOG": "info"
        }
      },
      "initialization_options": {
        "proton_extensions": true,
        "strict_mode": false,
        "max_errors": 100,
        "semantic_analysis": true
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "rust": {
          "analyzerTargetDir": true
        },
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "closureReturnTypeHints": {
            "enable": "always"
          }
        }
      }
    }
