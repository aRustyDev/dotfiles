{
  "context_servers": {
    "memory": {
      "source": "custom",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://mcp.localhost/memory/mcp",
        "--allow-http"
      ]
    },
    "sequentialthinking": {
      "source": "custom",
      "command": "docker",
      "args": ["exec", "-i", "thinking", "node", "dist/index.js"]
    },
    // "thinking-http": {
    //   "source": "custom",
    //   "command": "npx",
    //   "args": ["-y", "mcp-remote", "http://localhost:9001/mcp", "--allow-http"]
    // },
    "duckduckgo": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--network",
        "mcp-global_default",
        "mcp/duckduckgo"
      ]
    },
    "github": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--network",
        "mcp-global_default",
        "--env",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server",
        "stdio"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "op://Developer/zed-github-mcp/token"
      }
    },
    "gitlab": {
      "source": "custom",
      "command": "docker",
      "args": ["exec", "-i", "gitlab", "node", "dist/index.js"],
      "env": {
        "GITLAB_PERSONAL_ACCESS_TOKEN": "op://Developer/zed-mcp-gitlab/credential"
      }
    },
    "gitlab-sscm": {
      "source": "custom",
      "command": "docker",
      "args": ["exec", "-i", "SSCM", "node", "dist/index.js"],
      "env": {
        "GITLAB_API_URL": "https://sscm.gus.cisco.com/api/v4",
        "GITLAB_PERSONAL_ACCESS_TOKEN": "op://Cisco/2aesy5on3cb6z6opqieeautagi/token"
      }
    },
    "dockerhub": {
      "source": "custom",
      "command": "docker",
      "args": ["exec", "-i", "dockerhub", "node", "dist/index.js"],
      "env": {
        "HUB_PAT_TOKEN": "op://Cisco/koclujb733w4wcgafe4whlurwa/credential"
      }
    },
    "time": {
      "source": "custom",
      "command": "docker",
      "args": ["exec", "-i", "tool-time", "mcp-server-time"]
    },
    "simplechecklist": {
      "source": "custom",
      "command": "docker",
      "args": ["exec", "-i", "checklists", "node", "/app/mcp-server/index.js"]
    },
    "atlassian": {
      // https://support.atlassian.com/atlassian-rovo-mcp-server/docs/getting-started-with-the-atlassian-remote-mcp-server/
      "enabled": true,
      "url": "https://mcp.atlassian.com/v1/sse"
    },
    "markitdown": {
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-it",
        "--rm",
        "-v",
        "/home/user/data:/workdir",
        "markitdown-mcp:latest"
      ]
    }
  }
}
