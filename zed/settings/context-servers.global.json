        "--annotation",
        "scope:global",
        "--rm",
        "-e",
        "POSTGRES_URL",
        "-v",
        "$HOME/.local/share/mcp/postgres/global:/data",
        "mcp/postgres"
      ],
      "env": {
        "POSTGRES_URL": "postgresql://localhost:5432/global"
      }
    },
