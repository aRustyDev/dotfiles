    "just-search": {
      // "url": "https://tool.localhost/mcp",
      // "headers": {
      //   "X-Service": "just"
      // }
      "source": "custom",
      "command": "just-mcp",
      "args": [
        "search",
        "--database",
        "$HOME/.local/share/mcp/just/global_vector_search.db"
      ],
      "env": {
        "RUST_LOG": "info",
        "JUST_MCP_VECTOR_SEARCH": "true",
        "JUST_MCP_EMBEDDING_PROVIDER": "local"
      }
    },
