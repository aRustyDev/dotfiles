      // }
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--name",
        "mcp-mongodb",
        "--annotation",
        "scope:global",
        "--rm",
        "-e",
        "MDB_MCP_CONNECTION_STRING",
        "-v",
        "$HOME/.local/share/mcp/mongo/global:/data",
        "mcp/mongodb"
      ],
      "env": {
        // "MDB_MCP_CONNECTION_STRING": "mongodb+srv://$MONGO_USER:$MONGO_PASS@localhost:27017/global"
        "MDB_MCP_CONNECTION_STRING": "mongodb://localhost:27017/ami-eks"
      }
