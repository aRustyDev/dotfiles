        "-i",
        "--name",
        "mcp-grafana",
        "--annotation",
        "scope:global",
        "--rm",
        "-e",
        "GRAFANA_URL",
        "-e",
        "GRAFANA_API_KEY",
        "mcp/grafana",
        "--transport=stdio"
      ],
      "env": {
        "GRAFANA_URL": "http://localhost:3000",
        "GRAFANA_API_KEY": "op://employee/grafana/mcp-global/token"
      }
    },
    "atlassian": {
      // "url": "https://workflow.localhost/mcp",
      // "headers": {
      //   "X-Service": "atlassian"
      // }
      "source": "custom",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--name",
        "mcp-atlassian",
        "--annotation",
        "scope:global",
        "--rm",
        "-e",
        "CONFLUENCE_URL",
        "-e",
        "CONFLUENCE_USERNAME",
        "-e",
        "JIRA_URL",
        "-e",
        "JIRA_USERNAME",
        "-e",
        "CONFLUENCE_API_TOKEN",
        "-e",
        "CONFLUENCE_PERSONAL_TOKEN",
        "-e",
        "JIRA_API_TOKEN",
        "-e",
        "JIRA_PERSONAL_TOKEN",
        "mcp/atlassian"
      ],
      "env": {
        "CONFLUENCE_URL": "op://employee/confluence/url",
        "CONFLUENCE_USERNAME": "op://employee/confluence/email",
        "JIRA_URL": "op://employee/jira/url",
        "JIRA_USERNAME": "op://employee/jira/email",
        "CONFLUENCE_API_TOKEN": "op://employee/confluence/mcp-global/token",
        "CONFLUENCE_PERSONAL_TOKEN": "op://employee/confluence/mcp-global/token",
        "JIRA_API_TOKEN": "op://employee/jira/mcp-global/token",
        "JIRA_PERSONAL_TOKEN": "op://employee/jira/mcp-global/token"
      }
    },
