{{#engagements}}
Host {{shortname}}
    Hostname "op://{{vault}}/{{uuid}}/bastion/url"
    User "op://{{vault}}/{{uuid}}/bastion/user"
    DynamicForward "op://{{vault}}/{{uuid}}/bastion/dynamic-forward"
    ProxyJump {{shortname}}-bastion
    IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"

Host {{shortname}}-bastion
    Hostname "op://{{vault}}/{{uuid}}/bastion/url"
    User "op://{{vault}}/{{uuid}}/bastion/user"
    ProxyJump "op://{{vault}}/{{uuid}}/bastion/ip"
    IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
{{/engagements}}
