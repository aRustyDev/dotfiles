	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	
	# Complex aliases
	unstage = reset HEAD --
	last = log -1 HEAD
	visual = !gitk
	
	# Shell command aliases
	ignore = "!gi() { curl -sL https://www.toptal.com/developers/gitignore/api/$@ ;}; gi"

