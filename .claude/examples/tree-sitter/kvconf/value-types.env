# Example demonstrating all value types supported by the parser

# String Values
DOUBLE_QUOTED="This is a double-quoted string"
SINGLE_QUOTED='This is a single-quoted string'
WITH_SPACES="String with    multiple    spaces"
WITH_INTERPOLATION="Hello ${USER}, welcome to ${APP_NAME}"
EMPTY_STRING=""

# Boolean Values
FEATURE_ENABLED=true
DEBUG_MODE=false
# Note: only lowercase true/false are parsed as booleans
NOT_BOOL_CAPS=TRUE
NOT_BOOL_TITLE=True

# Numeric Values
PORT=3000
MAX_CONNECTIONS=100
NEGATIVE_NUMBER=-42
ZERO=0

# URI/URL Values
API_URL=https://api.example.com/v1
DATABASE_URL=postgresql://user:pass@localhost:5432/dbname
FTP_SERVER=ftp://files.example.com/public
FILE_PATH=file:///home/user/data.json
WEBSOCKET=ws://localhost:8080/socket

# Special Characters
WITH_EQUALS=key=value=with=equals
WITH_HASH=value#not-a-comment
WITH_SEMICOLON=value;also-not-a-comment
WITH_QUOTES=mix"ed'quotes"in'value

# Edge Cases
EMPTY=
ONLY_SPACES=   
TRAILING_SPACE=value   
LEADING_SPACE=   value
UNICODE=Hello ‰∏ñÁïå üåç