# Phase 3 Test: Primitive Types

# Booleans
DEBUG=true
PRODUCTION=false
VERBOSE=true

# Integers
PORT=3000
TIMEOUT=-1
MAX_CONNECTIONS=100
PRIORITY=+99

# Raw values (unchanged)
STRING_VALUE=hello world
MIXED_CASE=TrUe
NOT_BOOL=trueval
NOT_INT=123abc

# Comments work correctly after typed values
BOOL_WITH_COMMENT=true # this is a boolean
INT_WITH_COMMENT=42 # this is an integer
STRING_WITH_COMMENT="quoted" # this is a string

# Empty values still work
EMPTY=
EMPTY_WITH_COMMENT= # empty

# Special cases documented as limitations
DECIMAL=3.14          # Parsed as int(3) + error + int(14)
SCIENTIFIC=6.022e23   # Parsed as int(6) + error + identifier(e23)